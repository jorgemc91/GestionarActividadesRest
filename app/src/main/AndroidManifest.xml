    package="com.example.jorge.gestionactividades" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icono"
        android:label="@string/app_name"
        android:theme="@style/MiTema" >
        <activity
            android:name=".Principal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AnadirActividad"
            android:label="@string/accion_anadir" >
        </activity>
        <activity
            android:name=".Secundaria"
            android:label="@string/title_activity_secundaria" >
        </activity>
    </application>

</manifest>
